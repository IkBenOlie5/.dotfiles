// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "shift+alt+d",
        "command": "trailing-spaces.deleteTrailingSpaces"
    },
    {
        "key": "cmd+y",
        "command": "office.html.preview",
        "when": "editorTextFocus && editorLangId == 'html'"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-office.html.preview",
        "when": "editorTextFocus && editorLangId == 'html'"
    }
]